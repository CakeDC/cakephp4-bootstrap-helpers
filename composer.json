{
    "name": "holt59/cakephp3-bootstrap-helpers",
    "description": "Bootstrap Helpers for CakePHP 3.0",
    "keywords": ["CakePHP", "Bootstrap"],
    "license": "MIT",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|<6.0",
        "cakephp/cakephp-codesniffer": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Bootstrap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bootstrap\\Test\\": "tests/",
            "Bootstrap\\TestApp\\": "tests/test_app/"
        }
    },
    "extra": {
        "installer-name": "Bootstrap"
    }
}
